( prints the first 15 numbers of the Fibonacci sequence )
( ./cal fibonacci.aforth /dev/ttyUSB3 460800 )

bootstream async
( bootstream async-target)

node 708
: out18 0 out8 drop out8 out8
: out8 0 out1 7 for dup out1 2/ next 1
: out1 1 and 3 or !b 904 for unext ;
: exit 1 out8
: main
    io b! south a!
    15 for @ out18 drop next
    exit ;

node  608
north a!
1 0
: fib
over over + dup !
fib
